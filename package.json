    "node": "^22.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivansglazunov/hasyx.git"
  },
  "scripts": {
    "dev": "npm run doc:build && npm run unbuild && NODE_OPTIONS=\"--experimental-vm-modules\" next dev",
    "build": "npm run build:lib && npm run build:css && npm run doc:build && npm run build:templates && npm run ws && NODE_ENV=production next build",
    "build:lib": "tsc -p tsconfig.lib.json",
    "build:templates": "tsx lib/build-templates.ts",
    "build:css": "tailwindcss -i ./app/globals.css -o ./lib/styles.css",
    "doc:build": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/doc-public.ts",
    "unbuild": "find lib components hooks -type f \\( -name '*.js' -o -name '*.d.ts' \\) -not -path '*/types/*' -print -delete; rm -rf tsconfig.lib.tsbuildinfo _lib _components; echo 'âœ… Cleaned compiled files (types directory preserved)'",
    "build:next": "next build --turbopack",
    "client": "tsx lib/build-client.ts",
    "build:android": "npm run client && npm run cli -- assets && npx cap sync android",
    "open:android": "npx cap open android",
    "build:ios": "npm run client && npm run cli -- assets && npx cap sync ios",
    "open:ios": "npx cap open ios",
    "start": "NODE_ENV=production NODE_OPTIONS=\"--experimental-vm-modules\" npx -y hasyx start",
    "lint": "next lint --turbopack",
    "test": "npm run unbuild; NODE_OPTIONS=\"--experimental-vm-modules --max-old-space-size=4096\" jest --verbose --runInBand",
    "test:build": "jest build.test.ts",
    "postinstall": "npm run ws -- -y",
    "ws": "npx --yes next-ws-cli@latest patch -y",
    "migrate": "tsx lib/migrate.ts",
    "unmigrate": "tsx lib/unmigrate.ts",
    "events": "NODE_OPTIONS=\"--experimental-vm-modules\" npx tsx lib/events.ts",
    "schema": "npm run hasura:schema && npm run hasura:types",
    "hasura:schema": "tsx ./lib/hasura-schema.ts",
    "hasura:types": "graphql-codegen --config ./lib/hasura-types.ts",
    "npm-publish": "npm run build && npm publish",
    "prepack": "npm run build:lib && npm run build:css && npm run doc:build && npm run build:templates",
    "cli": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx ./lib/cli.ts",
    "ask": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/ask.ts",
    "js": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/js.ts",
    "tsx": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/tsx.ts",
    "ollama": "ollama",
    "logs": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/cli-hasyx.ts logs",
    "logs-diffs": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/cli-hasyx.ts logs-diffs",
    "logs-states": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/cli-hasyx.ts logs-states",
    "env": "NODE_OPTIONS=\"--experimental-vm-modules\" tsx lib/cli.ts env",
    "config": "npx tsx lib/config.tsx",
    "gitpod": "npx hasyx gitpod",
    "typedoc": "npx typedoc --out public/typedoc --entryPointStrategy expand --exclude '**/node_modules/**' --exclude '**/.next/**' lib components hooks"
  },
